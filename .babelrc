{
  "presets": [
    [
      "@babel/preset-env",
      {
        "corejs": {
          "version": 3,
          "proposals": true
        },
        "modules": false,
        "useBuiltIns": "entry"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": ["@babel/plugin-transform-runtime"],
  "env": {
    "test": {
      "plugins": ["@babel/plugin-transform-modules-commonjs"]
    }
  }
}
